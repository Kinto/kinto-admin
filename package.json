{
  "name": "kinto-admin",
  "version": "1.28.0",
  "description": "Kinto Web Administration Console in React.js",
  "scripts": {
    "build": "rimraf build && cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && shx cp html/prod.html build/index.html",
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "build:readme": "toctoc README.md -w",
    "cs-check": "prettier -l \"{src,test}/**/*.js\"",
    "cs-format": "prettier \"{src,test}/**/*.js\" --write",
    "flow-check": "flow check",
    "lint": "eslint src test",
    "publish-to-gh-pages": "npm run build && gh-pages --add --dist build/",
    "publish-to-npm": "npm run build:lib && npm publish",
    "start": "cross-env NODE_ENV=development node server/dev.js",
    "tdd": "npm run test -- --watch --bail",
    "test": "cross-env NODE_ENV=test jest",
    "test:ci": "npm run test -- --maxWorkers=4",
    "test-all": "npm run lint && npm run flow-check && npm run test",
    "test-all:ci": "npm run lint && npm install flow-typed && flow-typed install && npm run flow-check && npm run test:ci",
    "postinstall": "patch-package"
  },
  "main": "lib/index.js",
  "files": [
    "css",
    "images",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "npm": "^2.14.7",
    "node": ">=10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "atob": "^2.0.3",
    "bootstrap": "^4.5.2",
    "bootstrap-icons": "^1.0.0",
    "btoa": "^1.1.2",
    "codemirror": "^5.39.2",
    "connected-react-router": "^6.2.0",
    "diff": "^4.0.1",
    "express": "^4.14.0",
    "filesize": "^6.0.1",
    "history": "^4.9.0",
    "kinto-admin-form": "0.0.0-experimental-8183a9c80",
    "kinto-http": "^5.1.1",
    "patch-package": "^6.2.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.3.0",
    "react-bootstrap-icons": "^1.1.0",
    "react-breadcrumbs": "^2.1.6",
    "react-bs-notifier": "^6.0.0",
    "react-codemirror2": "^7.0.0",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.0",
