{
  "name": "kinto-admin",
  "version": "1.15.0",
  "description": "Kinto Web Administration Console in React.js",
  "scripts": {
    "build": "rimraf build && NODE_ENV=production node_modules/.bin/webpack --progress --config webpack.prod.js && cp html/prod.html build/index.html",
    "build:lib": "rimraf lib && NODE_ENV=production babel -d lib/ src/",
    "build:readme": "toctoc README.md -w",
    "cs-check": "prettier -l $npm_package_prettierOptions '{src,test,test-browser}/**/*.js'",
    "cs-format": "prettier $npm_package_prettierOptions '{src,test,test-browser}/**/*.js' --write",
    "flow-check": "node_modules/.bin/flow check",
    "lint": "node_modules/.bin/eslint src test test-browser",
    "publish-to-gh-pages": "npm run build && gh-pages --add --dist build/",
    "publish-to-npm": "npm run build:lib && npm publish",
    "start": "NODE_ENV=development node server/dev.js",
    "tdd": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/_mocha --require ignore-styles --require ./test/setup-jsdom.js 'test/**/*_test.js' --watch",
    "tdd-browser": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/_mocha 'test-browser/**/*_test.js' --watch",
    "test": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/_mocha --require ignore-styles --require ./test/setup-jsdom.js 'test/**/*_test.js'",
    "test-browser": "NODE_ENV=test node_modules/.bin/babel-node node_modules/.bin/_mocha 'test-browser/**/*_test.js'",
    "test-all": "npm run lint && npm run flow-check && npm run test"
  },
  "prettierOptions": "--jsx-bracket-same-line --trailing-comma es5 --semi",
  "main": "lib/index.js",
  "files": [
    "css",
    "images",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "npm": "^2.14.7",
    "node": ">=4"
  },
  "dependencies": {
    "atob": "^2.0.3",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.11.6",
    "bootstrap": "^3.3.6",
    "btoa": "^1.1.2",
    "codemirror": "^5.30.0",
    "diff": "^3.2.0",
    "express": "^4.14.0",
    "filesize": "^3.3.0",
    "kinto-http": "^4.3.3",
    "react": "^15.6.1",
    "react-breadcrumbs": "^1.3.16",
    "react-codemirror2": "^4.1.0",
    "react-dom": "^15.6.1",
    "react-jsonschema-form": "^0.48.2",
    "react-redux": "^5.0.7",
    "react-router": "^2.7.0",
    "react-router-redux": "4.0.8",
    "redux": "^3.0.4",
    "redux-saga": "^0.15.3",
    "rimraf": "^2.4.4",
    "timeago.js": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.9.1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.3.0",
    "css-loader": "^0.28.2",
    "eslint": "^4.18.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^1.1.10",
    "flow-bin": "^0.57.3",
    "gh-pages": "^1.0.0",
    "html": "1.0.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.0.0",
    "kinto-node-test-server": "1.0.0",
    "mocha": "^5.0.2",
    "mocha-generators": "^2.0.0",
    "nightmare": "^3.0.0",
    "prettier": "^1.9.2",
    "react-addons-test-utils": "^15.2.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.6",
    "sinon": "^4.4.1",
    "style-loader": "^0.18.1",
    "toctoc": "^0.2.3",
    "url-loader": "^1.0.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-middleware": "^3.0.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "greenkeeper": {
    "ignore": [
      "react-router"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinto/kinto-admin.git"
  },
  "keywords": [
    "kinto",
    "react",
    "admin",
    "json-schema"
  ],
  "author": "Nicolas Perriault <nperriault@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kinto/kinto-admin/issues"
  },
  "homepage": "https://github.com/kinto/kinto-admin#readme"
}
