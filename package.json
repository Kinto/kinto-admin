{
  "name": "kinto-admin",
  "version": "0.0.0-dev",
  "description": "Kinto Web Administration Console in React.js",
  "type": "module",
  "scripts": {
    "build": "npm run generate-version && vite build",
    "build:readme": "toctoc README.md -w",
    "build:kinto-single": "ASSET_PATH=/v1/admin KINTO_ADMIN_SINGLE_SERVER=1 npm run build",
    "cs-check": "prettier -l \"{src,test,bin}/**/*.{js,jsx,ts,tsx}\"",
    "cs-format": "prettier \"{src,test,bin}/**/*.{js,jsx,ts,tsx}\" --write",
    "generate-version": "node ./bin/generate-version.js",
    "lint": "eslint . --ext ts,tsx",
    "preview": "vite preview",
    "start": "npm run generate-version && vite",
    "tdd": "vitest",
    "test": "npm run generate-version && vitest --watch=false",
    "test:ci": "npm run test",
    "test-all": "npm run lint && npm run test",
    "test-all:ci": "npm run lint && npm run test:ci",
    "playwright": "playwright test",
    "playwright:install": "playwright install firefox"
  },
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@rjsf/bootstrap-4": "^5.13.2",
    "@rjsf/core": "^5.13.2",
    "@rjsf/validator-ajv8": "^5.13.2",
    "@uiw/codemirror-extensions-langs": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "bootstrap": "^4.6.2",
    "bootstrap-icons": "^1.11.1",
    "diff": "^5.0.0",
    "filesize": "^10.0.7",
    "history": "^4.10.1",
    "kinto-http": "^5.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.7",
    "react-bootstrap-icons": "^1.10.3",
    "react-bs-notifier": "^6.0.0",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux-first-history": "^5.0.12",
    "redux-saga": "^1.1.3",
    "timeago.js": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.14.177",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.2",
    "toctoc": "^0.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinto/kinto-admin.git"
  },
  "keywords": [
    "kinto",
    "react",
    "admin",
    "json-schema"
  ],
  "author": "Nicolas Perriault <nperriault@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kinto/kinto-admin/issues"
  },
  "homepage": "https://github.com/kinto/kinto-admin#readme"
}
