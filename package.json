{
  "name": "kinto-admin",
  "version": "2.1.1",
  "description": "Kinto Web Administration Console in React.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:readme": "toctoc README.md -w",
    "cs-check": "prettier -l \"{src,test}/**/*.{js,ts,tsx}\"",
    "cs-format": "prettier \"{src,test}/**/*.{js,ts,tsx}\" --write",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" \"test/**/*.{js,ts,tsx}\"",
    "preview": "vite preview",
    "publish-to-gh-pages": "cross-env ASSET_PATH=/kinto-admin/ npm run build && gh-pages --add --dist build/",
    "start": "vite",
    "tdd": "vitest",
    "test": "vitest --watch=false",
    "test:ci": "npm run test",
    "test-all": "npm run lint && npm run test",
    "test-all:ci": "npm run lint && npm run test:ci"
  },
  "engines": {
    "npm": ">=8",
    "node": ">=16"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@rjsf/bootstrap-4": "^5.13.2",
    "@rjsf/core": "^5.13.2",
    "@rjsf/validator-ajv8": "^5.13.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@uiw/codemirror-extensions-langs": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "atob": "^2.0.3",
    "bootstrap": "^4.6.2",
    "bootstrap-icons": "^1.11.1",
    "btoa": "^1.1.2",
    "diff": "^5.0.0",
    "express": "^4.14.0",
    "filesize": "^10.0.7",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^23.2.0",
    "kinto-http": "^5.1.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.7",
    "react-bootstrap-icons": "^1.10.3",
    "react-bs-notifier": "^6.0.0",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux-first-history": "^5.0.12",
    "redux-saga": "^1.1.3",
    "rimraf": "^5.0.0",
    "timeago.js": "^4.0.0",
    "vitest": "^1.1.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^12.1.5",
    "@types/history": "^5.0.0",
    "@types/lodash": "^4.14.177",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^9.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.0.1",
    "gh-pages": "^6.0.0",
    "html": "1.0.0",
    "prettier": "^3.0.2",
    "shx": "^0.3.2",
    "toctoc": "^0.4.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinto/kinto-admin.git"
  },
  "keywords": [
    "kinto",
    "react",
    "admin",
    "json-schema"
  ],
  "author": "Nicolas Perriault <nperriault@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kinto/kinto-admin/issues"
  },
  "homepage": "https://github.com/kinto/kinto-admin#readme"
}
