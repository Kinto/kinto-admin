{
  "name": "kinto-admin",
  "version": "0.10.0",
  "description": "Kinto Web Administration Console in React.js",
  "scripts": {
    "dist": "NODE_ENV=production webpack --optimize-minimize --config webpack.prod.js && cp index.prod.html dist/index.html",
    "lint": "eslint scripts test",
    "publish-to-gh-pages": "rimraf build && npm run dist && bin/kinto-admin build -c config/config.json -d build && gh-pages --dist build/",
    "start": "node devServer.js",
    "tdd": "npm test -- -w",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive --require ./test/setup-jsdom.js $(find test -name '*_test.js')"
  },
  "bin": {
    "kinto-admin": "bin/kinto-admin"
  },
  "files": [
    "bin",
    "dist"
  ],
  "engineStrict": false,
  "engines" : {
      "npm": "^2.14.7",
      "node": ">=4"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "jsonschema": "^1.0.2",
    "kinto": "^1.2.0",
    "node-fs-extra": "^0.8.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-jsonschema-form": "^0.16.1",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.4",
    "redux-simple-router": "0.0.10",
    "redux-thunk": "^1.0.0",
    "webpack": "^1.10.5",
    "yargs": "^3.31.0"
  },
  "devDependencies": {
    "babel": "^5.8.20",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.3.0",
    "css-loader": "^0.15.6",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "fake-indexeddb": "1.0.3",
    "gh-pages": "^0.4.0",
    "jsdom": "^7.1.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.0",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.4.4",
    "sinon": "^1.17.2",
    "style-loader": "^0.12.3",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinto/kinto-admin.git"
  },
  "keywords": [
    "kinto",
    "react",
    "admin",
    "json-schema"
  ],
  "author": "Nicolas Perriault <nperriault@mozilla.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kinto/kinto-admin/issues"
  },
  "homepage": "https://github.com/kinto/kinto-admin#readme"
}
