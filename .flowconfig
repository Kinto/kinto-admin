[ignore]
.*/node_modules/*
.*/test/.*

[include]
./node_modules/

[libs]
./flow-typed/
./interfaces/

[options]
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
module.name_mapper='.*\(.css\)' -> 'CSSModule'
module.name_mapper.extension='svg' -> '<PROJECT_ROOT>/src/SVGModule.js.flow'
module.system=haste
types_first=false

[lints]
deprecated-utility=off

